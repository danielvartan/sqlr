provider <- provider[filter]
id <- id[filter]
}
out <- list()
for (i in seq_along(provider)) {
pattern <- paste0("(?i)", provider[i])
out[[provider[i]]] <- list(provider = provider[i],
id = id[i],
pattern = pattern)
}
out
}
